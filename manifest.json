{
  "manifest_version": 3,
  "name": "Game Zone",
  "version": "1.0",
  "description": "Stressed from working? Take a break every hour with our super fun mini games!",
  "action": {
    "default_popup": "settings.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "alarms"]
}